define :intro do
  use_synth :chiplead
  play :f5, amp: 0.01
  sleep 0.5
  play :g5, amp: 0.02
  sleep 0.5
  play :a5, amp: 0.03
  sleep 0.5
  play :as5, amp: 0.04
  sleep 0.5
  play :b5, amp: 0.05
  sleep 0.5
  play :c6, amp: 0.06
  sleep 0.5
  play :d6, amp: 0.07
  sleep 1
  play :e5, amp: 0.08
  sleep 0.5
  play :f5, amp: 0.09
  sleep 0.5
  play :g5, amp: 0.1
  sleep 0.5
  play :gs5, amp: 0.2
  sleep 0.5
  play :a5, amp: 0.2
  sleep 0.5
  play :b5, amp: 0.3
  sleep 0.5
  play :c6, amp: 0.4
  sleep 1
  play :d6, amp: 0.5
  sleep 0.5
  play :e6, amp: 0.6
  sleep 0.5
  play :f6, amp: 0.7
  sleep 0.5
  play :d6, amp: 0.8
  sleep 0.5
  play :b5, amp: 0.9
  sleep 0.5
  play :c6, amp: 1
  sleep 0.5
  play :d6
  sleep 0.5
  play :b5
  sleep 0.5
  play :c6
  sleep 0.5
  play :b5
  sleep 0.5
  play :c6
  sleep 0.5
  play :d6
  sleep 0.5
  play :e6
  sleep 1
end

