define :bassE do
  use_synth :chipbass
  play :d3
  sleep 1.5
  play :g2
  sleep 2.5
  play :c3
  sleep 1.5
  play :f2
  sleep 1.5
  play :a2, amp: 1
  sleep 1
  play :b2, amp: 0.9
  sleep 1.5
  play :d3, amp: 0.8
  sleep 0.5
  play :e3, amp: 0.7
  sleep 1
  play :b2, amp: 0.6
  sleep 1
  play :a2, amp: 0.5
  sleep 1.5
  play :a3, amp: 0.4
  sleep 2.5
  play :d3, amp: 0.3
  sleep 1.5
  play :g2, amp: 0.2
  sleep 2.5
  play :c3, amp: 0.1
  sleep 1.5
  play :f2, amp: 0.09
  sleep 1.5
  play :a2, amp: 0.08
  sleep 1
  play :b2, amp: 0.07
  sleep 1.5
  play :d3, amp: 0.06
  sleep 0.5
  play :e3, amp: 0.05
  sleep 1
  play :b2, amp: 0.05
  sleep 1
  play :a2, amp: 0.05
  sleep 1
  play :e3, amp: 0.05
  sleep 1
  play :a2, amp: 0.05
  sleep 1
end

use_bpm 121
#Start
intro
#part 2
use_bpm 176
live_loop :mO do
  2.times do
    mTwo
    mOne
  end
  stop
end
live_loop :bass do
  2.times do
    bass
  end
  stop
end
wait 32
live_loop :guitar do
  gOne
  stop
end
wait 30
bassE

